{
  "name": "<%= appName %>",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel serve watch:**",
    "clean": "rimraf dist",
    "make": "npm-run-all make:**",
    "make:dir": "mkdirp dist dist/images dist/js dist/styles",
    "prebuild": "npm-run-all clean make",
    "build": "npm-run-all build:**",
    "build:html": "cpx src/index.html dist/",
    "build:images": "imagemin --optimizationLevel 4 src/images/*.* dist/images",
    "build:js": "webpack",
    "watch:js": "npm run build:js -- --watch",
    "preserve": "npm run build",
    "serve": "node server/server.js",
    "dev": "npm run build && browser-sync start --files='dist/**/*.*' --server dist",
    "prod": "NODE_ENV=production npm run serve",
    "lint": "eslint src/**/*.js*",
    "pretest": "npm run lint",
    "test": "karma start"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=5.0.0",
    "npm": ">=3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.2.3",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "browser-sync": "^2.7.10",
    "cpx": "^1.2.1",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.2.3",
    "extract-text-webpack-plugin": "^1.0.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.4.2",
    "npm-run-all": "^1.2.4",
    "postcss-loader": "^0.8.0",
    "precss": "^1.4.0",
    "rimraf": "^2.3.3",
    "sass-loader": "^3.2.0",
    "webpack": "^1.9.4",
    "webpack-notifier": "^1.2.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "body-parser": "^1.14.2",
    "history": "^1.17.0",
    "react": "^0.14.5",
    "react-dom": "^0.14.5",
    "react-router": "^1.0.3"
  }
}
